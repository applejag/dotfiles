{
  "yaml.schemas": {
    "https://iver-wharf.github.io/_static/wharf-ci-schema.json": [
      //"/home/kalle/dev/wharf/iver-wharf.github.io/docs/_static/wharf-ci-schema.json": [
        ".wharf-ci.yml"
      ]
  },
  "suggest.noselect": true,
  "languageserver": {
    "terraform": {
			"command": "terraform-ls",
			"args": ["serve"],
			"filetypes": [
				"terraform",
				"tf"
			],
			"initializationOptions": {},
			"settings": {}
    }
  },
  //"diagnostic.displayByAle": true,
  "Lua.workspace.library": [
    "/home/kalle/Documents/CC-Tweaked-EmmyLua"
  ],
